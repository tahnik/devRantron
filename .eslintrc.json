{
	"parser": "babel-eslint",
	"extends": "google",
	"env": {
		"es6": true,
		"node": true,
		"browser": true
	},
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true
		},
		"sourceType": "module",
		"ecmaVersion": 6
	},
	"plugins": [
		"react",
		"jsx-a11y",
		"import"
	],
	"rules": {
		"import/no-extraneous-dependencies": "off"
	},
	"settings": {
		"import/resolver": {
			"webpack": {
				"config": "./configs/webpack.dev.config.js"
			}
		}
	}
}
